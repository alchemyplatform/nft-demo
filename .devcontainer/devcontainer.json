{
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "features": {
  },
  "customizations": {
    "codespaces": {
      "repositories": {
        "alchemyplatform/nft-demo": {
          "permissions": "read-all"
        }
      }
    },
    "vscode": {
      "extensions": [
        "ms-vscode.live-server"
      ]
    }
  },
  "postAttachCommand": {
    "server": "yarn dev"
  },
  "portsAttributes": {
    "3000": {
      "label": "Application",
      "onAutoForward": "openPreview"
    }
  }
}
